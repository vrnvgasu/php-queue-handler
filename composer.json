{
  "name": "vrnvgasu/php-rabbit-handler",
  "description": "Handle queues",
  "license": "MIT",
  "type": "library",
  "keywords": ["rabbitmq", "message", "queue"],
  "require": {
    "php": ">=8.0",
    "php-amqplib/php-amqplib": ">=3.2"
  },
  "authors": [
    {
      "name": "Dmitrii",
      "role": "Original Maintainer"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.12.18"
  },
  "autoload": {
    "psr-4": {
      "Vrnvgasu\\PhpRabbitHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "./vendor/bin/phpunit -v --coverage-text --colors=always"
    ]
  }
}
